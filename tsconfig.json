{
    "compilerOptions": {
        "baseUrl": "./src",
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "es6",
        "allowSyntheticDefaultImports": true,
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "noEmit": true,
        "paths": {
            // "components/*": ["src/components/*"],
            "@components/*": ["components/*"],
            "@routes/*": ["routes/*"],
            "~/*": ["/*"]
        },
    },
    "exclude": [
        "node_modules",
        "dist",
        "antd"
    ]
}
